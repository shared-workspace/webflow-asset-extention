{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    
    "dev": "NODE_ENV=development nuxt dev",
    "ext:server": "SERVER_DEV=true nuxt dev",
    "ext:client": "WEBFLOW_BUILD=true yarn generate",
    "ext:client-dev": "nodemon --watch ./src --ext ts,vue --exec 'yarn ext:client'",
    "ext:dev": "concurrently -k \"yarn ext:server\" \"yarn ext:client-dev\"",
    "ext:webflow": "webflow extension serve"
  },
  "dependencies": {
    "nuxt": "^3.13.0",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.7",
    "@webflow/designer-extension-typings": "^2.0.2",
    "axios": "^1.7.7",
    "concurrently": "^9.0.1",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.7",
    "openai": "^4.66.1",
    "webflow-api": "^2.4.0"
  }
}
